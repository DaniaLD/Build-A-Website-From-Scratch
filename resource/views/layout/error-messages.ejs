<% if(errors.length) { %>
    <div class="alert alert-danger alert-dismissible">
            <button type="button" class="close" data-dismiss="alert" aria-label="close">
                    <span>x</span>
            </button>
            <% errors.forEach(element => { %>
                    <%= element %> <br>
            <% }) %>
    </div>
<% } %>